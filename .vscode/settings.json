                    "foreground": "#dc1b1b"
                }
            },
            {
                "scope": "variable.parameter.nlp",
                "settings": {
                    "foreground": "#f8fa87"
                }
            },
            {
                "scope": "keyword.attribute.nlp",
                "settings": {
                    "foreground": "#f293fb"
                }
