                    "foreground": "#93bffb"
                }
            },
            {
                "scope": "keyword.fired.tree",
                "settings": {
                    "foreground": "#dc1b1b"
                }
            },
            {
                "scope": "keyword.rewrite.tree",
                "settings": {
                    "foreground": "#f8fa87"
                }
            },
            {
                "scope": "keyword.node.tree",
                "settings": {
                    "foreground": "#4e8e3c"
