{
    "name": "nlp",
    "displayName": "NLP",
    "description": "NLP++",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    ],
    "main": "./out/extension.js",
    "contributes": {
                }
        ],
        "languages": [
            {
                "id": "nlp",
                "aliases": [
                    "NLP++",
                    "nlp"
                ],
                "extensions": [
                    ".nlp",
                    ".nlppp",
                    ".pat"
                ],
                "configuration": "./nlp-configuration.json"
            },
            {
                "id": "seq",
                "extensions": [
                    "seq"
                ],
                "aliases": [
                    "SEQ",
                    "seq"
                ],
                "configuration": "./seq.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nlp",
                "scopeName": "source.nlp",
                "path": "./syntaxes/nlp.tmLanguage.json"
            },
            {
                "language": "seq",
                "scopeName": "source.seq",
                "path": "./syntaxes/seq.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "scopeName": "source.nlp",
                "path": "./snippets/nlp.json"
            }
        ],
        "colors": [
            {
                "id": "keyword.region.nlp",
                "description": "Color for NLP regions.",
                "defaults": {
                    "dark": "foreground",
                    "light": "foreground",
                    "highContrast": "#010203"
                }
            }
        ],
        "commands": [
            {
                "command": "nlp.analyze",
                "title": "NLP: Analyze Text",
                "description": "Run the analyzer on text"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceLangId == seq",
                    "command": "nlp.analyze",
                    "group": "sequence"
                },
                {
                    "when": "resourceExtname == .txt",
                    "command": "nlp.analyze",
                    "group": "sequence"
                }
            ],
            "editor/context": [
                {
                    "when": "resourceExtname == .seq",
                    "command": "nlp.analyze",
                    "group": "nlp"
                },
                {
                    "when": "resourceExtname == .pat || NresourceExtname == .nlp",
                    "command": "nlp.analyze",
                    "group": "nlp"
                }
            ]
        }
    }
}